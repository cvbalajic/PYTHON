a = int(input())
s = [input() for _ in range(a)]
dic = []
tm=[]
ass=[]
lab=[]
avg=[]
for i in s:
    co=0
    b = i.split(" ")
    sum=0
    for j in b:
        if j.isalpha():
            w = j
        elif j.isdigit():
            co=co+1
            sum+=int(j)
            if(co==1):
                tm.append(j)
            elif(co==2):
                ass.append(j)
            elif(co==3):
                lab.append(j)
            else:
                pass
        else:
            pass
    dic.append(w)
    avg.append(int(sum))
#higest average score
ts1=0
for i in range(0,len(dic)):
    if avg[i]>ts1:
        w2=dic[i]
        ts1=avg[i]
    elif(avg[i]==ts1):
        print(dic[i],end=" ")
print(w2) 
