n=int(input())
lst=[]
count=0
flag=0
for i in range(0,n):
    ele=int(input())
    lst.append(ele)
k=int(input())
for i in range(0,n):
    if(lst[i]==k):
        print(k,"is present at location %d."%(i+1))
        count+=1
        flag=1
if(flag==0):
    print(k,"is not present in the array.")
else:
    print(k,"is present",count,"times in the array.")

