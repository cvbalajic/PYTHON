def main():
    try:
        num1 = float(input())
        num2 = float(input())
        
        result = num1 / num2
        print(result)
    
    except ValueError:
        print("Error: Non-numeric input provided.")
    except ZeroDivisionError:
        print("Error: Cannot divide or modulo by zero.")

if __name__ == "__main__":
    main()
